
import java.io.*;
import java.util.*;

public class problem1 {
    private static String rotateLeft(String s) {
        if (s.length() == 0) return s;
        return s.substring(1) + s.charAt(0);
    }

    private static String rotateRight(String s) {
        if (s.length() == 0) return s;
        return s.charAt(s.length() - 1) + s.substring(0, s.length() - 1);
    }

    public static void main(String[] args) throws Exception {
        List<String> dirs = new ArrayList<>();
        List<String> grid = new ArrayList<>();

    
        BufferedReader br = new BufferedReader(new FileReader("inputs/directions.txt"));
        String line = br.readLine();
        if (line != null) {
            for (String d : line.trim().split("\\s+")) {
                dirs.add(d);
            }
        }
        br.close();


        br = new BufferedReader(new FileReader("inputs/grid.txt"));
        while ((line = br.readLine()) != null) {
            line = line.trim();
            if (!line.isEmpty()) grid.add(line);
        }
        br.close();

        List<String> newRows = new ArrayList<>();
        for (int i = 0; i < grid.size(); i++) {
            String row = grid.get(i);
            String d = dirs.get(i);

            if (d.equals("R")) {
                newRows.add(rotateRight(row));
            } else {
                newRows.add(rotateLeft(row));
            }
        }

        int mid = newRows.size() / 2;
        String midRow = newRows.get(mid);

        int sum = 0;
        for (char c : midRow.toCharArray()) {
            sum += (int) c;
        }

        System.out.println(sum);
    }
}
